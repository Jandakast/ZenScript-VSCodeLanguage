{
    "Hide": {
        "prefix": "hide",
        "body": [
            "NEI.hide(${0:<item>});"
        ],
        "scope": "zenscript",
        "description": "Hide item from NEI"
    },
    "AddEntry": {
        "prefix": "addentry",
        "body": [
            "NEI.addEntry(${1::<item>}.withTag(${2:NBTTag}));"
        ],
        "scope": "zenscript",
        "description": "Change item NBT in NEI"
    },
    "OverrideName": {
        "prefix": "overridename",
        "body": [
            "NEI.overrideName(${1::<item>}, \"${2:Name}\");"
        ],
        "scope": "zenscript",
        "description": "Change item Name in NEI"
    }
}